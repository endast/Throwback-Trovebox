{{#result}}
<a href="#"><img alt="{{filenameOriginal}}" src="{{path200x200xCR}}" data-toggle="modal" data-target="#{{id}}" class="img-thumbnail img-responsive"></a>

<div class="modal fade" id="{{id}}" tabindex="-1" role="dialog" aria-labelledby="{{id}}_label" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="{{id}}_label">{{filenameOriginal}}</h4>
      </div>
      <div class="modal-body">
        <a target="_blank" href="{{url}}"><img class="center-block  img-responsive" src="{{path600x600}}"></a>
      </div>

    </div>
  </div>
</div>
{{/result}}
